{
  "name": "audio-only-youtube",
  "displayName": "Audio Only Youtube",
  "version": "0.7.0",
  "description": "Enables you to disable video streams on youtube and listen audio",
  "main": "index.js",
  "repository": "git@github.com:Ashish-Bansal/audio-only-youtube.git",
  "author": "Ashish Bansal <bansal.ashish096@gmail.com>",
  "license": "GPLv3",
  "private": true,
  "scripts": {
    "build:dev": "concurrently --kill-others 'npm:build:manifest' 'npm:parcel:dev'",
    "build:prod": "concurrently --kill-others 'npm:build:manifest' 'npm:parcel:prod'",
    "build:manifest": "node manifest.js",
    "parcel:dev": "parcel ./src/*/**/*.*",
    "parcel:prod": "parcel build ./src/*/**/*.*",
    "commit": "npx git-cz",
    "commitmsg": "npx commitlint -E GIT_PARAMS",
    "precommit": "npx lint-staged",
    "postcommit": "git update-index --again"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@types/chrome": "0.0.71",
    "commitizen": "^2.10.1",
    "concurrently": "^3.6.1",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^0.14.3",
    "imagemin-lint-staged": "^0.3.0",
    "lint-staged": "^7.2.0",
    "parcel": "^1.9.7",
    "prettier": "1.14.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
